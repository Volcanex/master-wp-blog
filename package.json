{
  "name": "wordpress-business-template",
  "version": "1.0.0",
  "description": "A professional WordPress business template optimized for small businesses and Hostinger hosting",
  "main": "index.js",
  "scripts": {
    "dev": "npm run watch",
    "watch": "concurrently \"npm run watch:css\" \"npm run watch:js\"",
    "watch:css": "sass --watch wp-content/themes/astra-child/assets/scss:wp-content/themes/astra-child/assets/css --style=expanded --source-map",
    "watch:js": "webpack --mode development --watch",
    "build": "npm run build:css && npm run build:js",
    "build:css": "sass wp-content/themes/astra-child/assets/scss:wp-content/themes/astra-child/assets/css --style=compressed --no-source-map",
    "build:js": "webpack --mode production",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint \"wp-content/themes/astra-child/**/*.{css,scss}\"",
    "lint:js": "eslint \"wp-content/themes/astra-child/**/*.js\"",
    "fix": "npm run fix:css && npm run fix:js",
    "fix:css": "stylelint \"wp-content/themes/astra-child/**/*.{css,scss}\" --fix",
    "fix:js": "eslint \"wp-content/themes/astra-child/**/*.js\" --fix",
    "test": "echo \"No tests specified\" && exit 0",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f wordpress",
    "docker:shell": "docker-compose exec wordpress bash",
    "docker:db": "docker-compose exec db mysql -u wordpress -p",
    "deploy:staging": "echo \"Configure staging deployment script\"",
    "deploy:production": "echo \"Configure production deployment script\"",
    "backup": "echo \"Configure backup script\"",
    "setup:local": "npm install && npm run setup:env",
    "setup:env": "cp .env.example .env && echo \"Please update .env with your local settings\"",
    "security:check": "npm audit && echo \"Check WordPress core and plugins for updates\"",
    "optimize:images": "imagemin wp-content/themes/astra-child/assets/images/**/* --out-dir=wp-content/themes/astra-child/assets/images/optimized"
  },
  "keywords": [
    "wordpress",
    "business",
    "template",
    "theme",
    "hostinger",
    "astra",
    "child-theme",
    "responsive",
    "seo",
    "performance"
  ],
  "author": {
    "name": "Business Template Developer",
    "email": "developer@example.com",
    "url": "https://example.com"
  },
  "license": "GPL-2.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/wordpress-business-template"
  },
  "bugs": {
    "url": "https://github.com/yourusername/wordpress-business-template/issues"
  },
  "homepage": "https://github.com/yourusername/wordpress-business-template#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "eslint-config-wordpress": "^2.0.0",
    "imagemin": "^8.0.1",
    "imagemin-cli": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "sass": "^1.69.0",
    "sass-loader": "^13.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-wordpress": "^17.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {},
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "config": {
    "theme_path": "wp-content/themes/astra-child",
    "assets_path": "wp-content/themes/astra-child/assets",
    "build_path": "wp-content/themes/astra-child/assets/dist"
  },
  "wordpress": {
    "theme": "astra-child",
    "parent_theme": "astra",
    "min_wp_version": "5.0",
    "tested_up_to": "6.4",
    "requires_php": "7.4"
  },
  "development": {
    "local_url": "http://localhost:8000",
    "docker_url": "http://localhost:8000",
    "proxy_url": "http://localhost:3000"
  },
  "deployment": {
    "staging": {
      "url": "https://staging.yourdomain.com",
      "path": "/path/to/staging"
    },
    "production": {
      "url": "https://yourdomain.com",
      "path": "/path/to/production"
    }
  },
  "hosting": {
    "provider": "Hostinger",
    "server": "LiteSpeed",
    "php_version": "8.1",
    "mysql_version": "8.0"
  }
}